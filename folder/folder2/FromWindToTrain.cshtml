  <script>

  // RESET FUNCTION
  var reseTT = function() {
      vivusLine1.reset();
      vivusLine2.reset();
      vivusLine3.reset();
      vivusLine4.reset();
      vivusLine5.reset();
  };

    $(document).ready(function() {

      var mobCheck = /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);

      if(mobCheck){
        $('audio').remove();
        $('.sidebar').removeClass('sidebar-desktop').addClass('sidebar-tablet');
        // $('.dot-link').addClass('mobile-dot-link').removeClass('dot-link-desktop');
      } else {

      }

      // FULLPAGE ---------------------------------------------------------------
      $('#fullpage').fullpage({

        // ONLOAD -------------------------------------------
        afterLoad: function(anchorLink, index){

          reseTT();

            if(index == 8){
              vivusLine1.play();
            }

            if(index == 9){
              vivusLine2.play();
            }

            if(index == 11){
              vivusLine3.play();
            }

            if(index == 19){
              vivusLine4.play();
            }

            if(index == 21){
              vivusLine5.play();
            }

            var perC = '0';

              if(index >= 1  && index <= 5 ){ perC = '0' }
              if(index >= 6  && index <= 11){ perC = '20%' }
              if(index >= 12 && index <= 14){ perC = '40%' }
              if(index >= 15 && index <= 18){ perC = '60%' }
              if(index >= 18 && index <= 22){ perC = '80%' }
              if(index >= 23 && index <= 24){ perC = '100%' }

            $('.line-marker-2').animate({
              top: perC
            }, 300);

            $('.line-green').animate({
              height: perC
            }, 300, function(){

              if(index >= 6){
                $('.line-dot-2').css('background-color', '#F4D23E');
              } else {
                $('.line-dot-2').css('background-color', '#364081');
              }

              if(index >= 12){
                $('.line-dot-3').css('background-color', '#F4D23E');
              } else {
                $('.line-dot-3').css('background-color', '#364081');
              }

              if(index >= 15){
                $('.line-dot-4').css('background-color', '#F4D23E');
              } else {
                $('.line-dot-4').css('background-color', '#364081');
              }

              if(index >= 18){
                $('.line-dot-5').css('background-color', '#F4D23E');
              } else {
                $('.line-dot-5').css('background-color', '#364081');
              }

              if(index >= 23){
                $('.line-dot-6').css('background-color', '#F4D23E');
              } else {
                $('.line-dot-6').css('background-color', '#364081');
              }

            });


  				},
        // ONLEAVE -------------------------------------------
        onLeave: function(index, nextIndex, direction){


        }
      });

      $('.next-page').click(function(){
        $.fn.fullpage.moveSectionDown();
      });


      $('.thumb1').click(function(){
        $('.vid').html('<iframe class="lightbox-vid video1" src="https://www.youtube.com/embed/3Zp4o703JEY" frameborder="0" allowfullscreen></iframe>');
        $('.lightbox-container').fadeIn();
      });

      $('.thumb2').click(function(){
        $('.vid').html('<iframe class="lightbox-vid video1" src="https://www.youtube.com/embed/pliMcWDQbHQ" frameborder="0" allowfullscreen></iframe>');
        $('.lightbox-container').fadeIn();
      });

      $('.thumb3').click(function(){
        $('.vid').html('<iframe class="lightbox-vid video1" src="https://www.youtube.com/embed/ZR_IuMwpuMk" frameborder="0" allowfullscreen></iframe>');
        $('.lightbox-container').fadeIn();
      });

      $('.lightbox-container').click(function(){
        $('.lightbox-container').fadeOut();
        $('iframe').attr('src', $('iframe').attr('src'));
      });

      $('.close-lightbox').click(function(){
        $('.lightbox-container').fadeOut();
        $('iframe').attr('src', $('iframe').attr('src'));
      });

      // SIDEBAR OPEN
      var sideOpen = function(){
        $('.sidebar').stop().animate({
          width: 630
        }, 150);

        $('.icon-marker').fadeOut('fast');

        vivusIcon1.play();
        vivusIcon2.play();
        vivusIcon3.play();
        vivusIcon4.play();
        vivusIcon5.play();
        vivusIcon6.play();

        $('.sidebar-screen').fadeIn();
      };

      // SIDEBAR CLOSE FUNCTION
      var sideClose = function(){
        $('.sidebar').stop().animate({
          width: 100
        }, 150, function(){
          vivusIcon1.reset();
          vivusIcon2.reset();
          vivusIcon3.reset();
          vivusIcon4.reset();
          vivusIcon5.reset();
          vivusIcon6.reset();
        });

        $('.icon-marker').fadeIn();
        $('.sidebar-screen').fadeOut();
      };

      // SIDEBAR MOBILE OPEN
      var sideOpenMobile = function(){
        $('.sidebar').stop().animate({
          width: '100%'
        }, 150);

        vivusIcon1.play();
        vivusIcon2.play();
        vivusIcon3.play();
        vivusIcon4.play();
        vivusIcon5.play();
        vivusIcon6.play();
      };

      // SIDEBAR MOBILE CLOSE FUNCTION
      var sideCloseMobile = function(){
        $('.sidebar').stop().animate({
          width: 50
        }, 150, function(){
          vivusIcon1.reset();
          vivusIcon2.reset();
          vivusIcon3.reset();
          vivusIcon4.reset();
          vivusIcon5.reset();
          vivusIcon6.reset();
          $('.sidebar-screen').fadeOut();
        });
      };

      // SIDEBAR MOUSE OVER
      $('.sidebar-desktop').mouseover(function(){
        sideOpen();
      });

      // SIDEBAR MOUSE OUT
      $('.sidebar-screen').mouseover(function(){
        sideClose();
      });

      // SIDEBAR MOUSE OVER TABLET
      $('.sidebar-opener').click(function(){
        sideOpen();
      });

      // SIDEBAR CLICK OUT TABLET
      $('.sidebar-screen').click(function(){
        sideClose();
      });

      // SIDEBAR MOUSE OVER MOBILE
      $('.sidebar-opener-mobile').click(function(){
        sideOpenMobile();
      });


      // SIDEBAR BUTTON CLICK
      $('.dot-link').click(function(){

        var mobCheck = /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);
        if(mobCheck){

          var indexNo = $(this).data('index');
          sideCloseMobile();
          $.fn.fullpage.moveTo(indexNo);

        } else {

          var indexNo = $(this).data('index');
          sideClose();
          $.fn.fullpage.moveTo(indexNo);
        }

      });


      $('.share-container').mouseenter(function(){
        $('.share-hider').stop().delay(200).fadeIn();
        $('.share-icon-main').stop().fadeOut(100);
      }).mouseleave(function(){
        $('.share-hider').stop().fadeOut(50);
        $('.share-icon-main').stop().delay(200).fadeIn(100);
      });

      $('.share-buttons li').mouseenter(function(){
        var shareT = $(this).data('social');
        $('.share-text').html(shareT);
      }).mouseleave(function(){
        $('.share-text').html('Delen');
      });

      //audio on hover
      var audio1 = $("#hoverclick")[0];
      $(".hovy").click(function() {
        audio1.play();
      });
      // // $(".hovy").mouseleave(function() {
      // //   audio1.pause();
      // //   audio1.currentTime = 0;
      // });

